MERGE (s1:Serie {id: 200}) SET s1.titulo = "Dark", s1.ano_inicio = 2017
MERGE (s2:Serie {id: 201}) SET s2.titulo = "Stranger Things", s2.ano_inicio = 2016
MERGE (s3:Serie {id: 202}) SET s3.titulo = "Breaking Bad", s3.ano_inicio = 2008
MERGE (s4:Serie {id: 203}) SET s4.titulo = "The Witcher", s4.ano_inicio = 2019
MERGE (s5:Serie {id: 204}) SET s5.titulo = "The Mandalorian", s5.ano_inicio = 2019

WITH u1,u2,
     g1,g2,g3,g4,g5,
     a1,a2,a3,a4,a5,
     d1,d2,d3,
     f1,f2,f3,f4,f5,
     s1,s2,s3,s4,s5

MERGE (f1)-[:TEM_GENERO]->(g1)
MERGE (f1)-[:TEM_GENERO]->(g4)
MERGE (f2)-[:TEM_GENERO]->(g1)
MERGE (f2)-[:TEM_GENERO]->(g2)
MERGE (f3)-[:TEM_GENERO]->(g1)
