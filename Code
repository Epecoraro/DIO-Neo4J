MERGE (f4)-[:TEM_GENERO]->(g2)
MERGE (f4)-[:TEM_GENERO]->(g4)
MERGE (f5)-[:TEM_GENERO]->(g1)
MERGE (f5)-[:TEM_GENERO]->(g5)

MERGE (s1)-[:TEM_GENERO]->(g1)
MERGE (s1)-[:TEM_GENERO]->(g4)
MERGE (s2)-[:TEM_GENERO]->(g1)
MERGE (s2)-[:TEM_GENERO]->(g5)
MERGE (s3)-[:TEM_GENERO]->(g3)
MERGE (s3)-[:TEM_GENERO]->(g4)
MERGE (s4)-[:TEM_GENERO]->(g5)
MERGE (s4)-[:TEM_GENERO]->(g3)
MERGE (s5)-[:TEM_GENERO]->(g1)
MERGE (s5)-[:TEM_GENERO]->(g2)

MERGE (a1)-[:ATUOU]->(f1)
MERGE (a2)-[:ATUOU]->(f2)
MERGE (a3)-[:ATUOU]->(f3)
MERGE (a1)-[:ATUOU]->(f4)
MERGE (a4)-[:ATUOU]->(s2)
MERGE (a5)-[:ATUOU]->(s3)

MERGE (d1)-[:DIRIGIU]->(f1)
MERGE (d1)-[:DIRIGIU]->(f3)
MERGE (d1)-[:DIRIGIU]->(f4)
MERGE (d2)-[:DIRIGIU]->(f2)
MERGE (d3)-[:DIRIGIU]->(s3)

MERGE (u1)-[:ASSISTIU {avaliou_nota: 5}]->(f1)
MERGE (u1)-[:ASSISTIU {avaliou_nota: 4}]->(f3)
MERGE (u1)-[:ASSISTIU {avaliou_nota: 5}]->(s1)
MERGE (u1)-[:ASSISTIU {avaliou_nota: 4}]->(s2)
MERGE (u2)-[:ASSISTIU {avaliou_nota: 5}]->(f2)
MERGE (u2)-[:ASSISTIU {avaliou_nota: 4}]->(f4)
